(or
    (some
        events
        (fn (let $1
            (or
                (and
                    (== type "CovidTest")
                    (== testType "PCR")
                    (== selfTest false)
                    (<=
                        0
                        (timeSince testedAt)
                        (hours 72)
                    )
                )
                (and
                    (== type "CovidTest")
                    (== testType "AntiGen")
                    (== selfTest false)
                    (<=
                        0
                        (timeSince testedAt)
                        (hours 48)
                    )
                )
                (and
                    (== type "CovidTest")
                    (== selfTest true)
                    (<=
                        0
                        (timeSince testedAt)
                        (hours 24)
                    )
                )
                (and
                    (== type "Vaccination")
                    (in vaccine [
                        "EU/1/20/1528"
                        "EU/1/20/1507"
                        "EU/1/21/1529"
                    ])
                    (<=
                        0
                        (timeSince vaccinatedAt)
                        (days 270)
                    )
                    (== doseNumber 2)
                )
                (and
                    (== type "Vaccination")
                    (in vaccine [
                        "EU/1/20/1525"
                    ])
                    (<=
                        (days 21)
                        (timeSince vaccinatedAt)
                        (days 270)
                    )
                    (== doseNumber 1)
                )
                (and
                    (== type "Recovery")
                    (<
                        (timestamp infectedAt)
                        (timestamp recoveredAt)
                    )
                    (<=
                        0
                        (timeSince recoveredAt)
                        (days 180)
                    )
                )
            )
        ))
    )
    (let
        {
            vaccinations: (map
                (filter
                    events
                    (fn (let $1
                        (and
                            (== type "Vaccination")
                            (in vaccine [
                                "EU/1/20/1528"
                                "EU/1/20/1507"
                                "EU/1/21/1529"
                                "EU/1/20/1525"
                            ])
                            (<=
                                0
                                (timeSince vaccinatedAt)
                                (days 270)
                            )
                            (>= doseNumber 1)
                        )
                    ))
                )
                (fn (timestamp (arg 1 vaccinatedAt)))
            )
            infections: (map
                (filter
                    events
                    (fn (let $1
                        (and
                            (== type "Recovery")
                            (<=
                                0
                                (timeSince recoveredAt)
                            )
                            (<
                                (timestamp infectedAt)
                                (timestamp recoveredAt)
                            )
                        )
                    ))
                )
                (fn (timestamp (arg 1 infectedAt)))
            )
        }
        (some
            vaccinations
            (fn
                vaccinatedAt
                (some
                    infections
                    (fn
                        infectedAt
                        (>=
                            (- vaccinatedAt infectedAt)
                            (days 21)
                        )
                    )
                )
            )
        )
    )
)
